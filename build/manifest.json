{
  "short_name": "browserWallet",
  "name": "Solar Browser Wallet",
  "content_security_policy": {
    "extension_pages":"script-src 'self'; object-src 'self'"
    },
  "icons": {
    "16": "isotype_square.png",
    "32": "isotype_square.png",
    "48": "isotype_square.png",
    "96": "isotype_square.png",
    "128": "isotype_square.png"
  },
  "action": {
    "default_icon": {
      "16": "isotype_square.png",
      "32": "isotype_square.png"
      }
  },
  "permissions": [
    "storage",
    "cookies",
    "unlimitedStorage",
    "notifications"
  ],
  "background": {
    "service_worker": "armor.js"
  }, 
  "version": "1.0.1",
  "manifest_version": 3
}
